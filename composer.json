{
    "name": "iperamuna/laravel-changelog",
    "description": "A Command Line Laravel Package to Manage Changelog.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Indunil Peramuna",
            "email": "iperamuna@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "laravel/prompts": "^0.3.6",
        "league/commonmark": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Iperamuna\\LaravelChangelog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iperamuna\\LaravelChangelog\\LaravelChangelogServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
